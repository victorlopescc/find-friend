{
  "name": "find-friend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "test:create": "npm link ./prisma/vitest-environment-prisma",
    "test:install": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/tests/unit",
    "pretest:e2e": "run-s test:create test:install",
    "test:e2e": "vitest run --dir src/tests/e2e",
    "test:watch": "vitest --dir src/tests/e2e",
    "test:cover": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "build": "tsup src --out-dir build"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/jwt": "8.0.0",
    "@prisma/client": "5.10.2",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "fastify": "4.26.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.11.25",
    "@types/supertest": "6.0.2",
    "npm-run-all": "4.1.5",
    "prisma": "5.10.2",
    "supertest": "6.3.4",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "5.4.2",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  }
}
